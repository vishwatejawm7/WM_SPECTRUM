#include<iostream>
#include<string.h>
using namespace std;
int main()
{
    char str[100];
    cout << "Enter the string : ";
    cin >> str;
    int l = strlen(str), count{0};
    for(int i=0; i<l; i++)
    {
        if(str[i] == 'a' || str[i] == 'e' || str[i] == 'e' || str[i] == 'i' || str[i] == 'o' || str[i] == 'u' || str[i] == 'A' 
        || str[i] == 'E' || str[i] == 'I' || str[i] == 'O' || str[i] == 'U')
        {
            count++;
        }
    }
    cout << "The number of vowels : " << count;
    return 0;
}
