#include<iostream>
using namespace std;

int main()
{
    int a;
    int arr[100];
    cout << "Enter the size of the array : ";
    cin >> a;
    for(int i=0; i<a; i++)
    {
        cout << "Enter the element " << i << " of the array : ";
        cin >> arr[i];
    }
    int min = arr[0];
    for(int i=1; i<a; i++)
    {
        if(arr[i] < min)
        {
            min = arr[i];
        }
    }
    cout << "Smallest element of the array is : " << min << endl;
    return 0;
}
