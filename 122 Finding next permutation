#include <bits/stdc++.h>
using namespace std;
int main()
{
  string s;
    cout << "Enter a permutation : " << endl;
    cin >> s;
    bool val = next_permutation(s.begin(), s.end());
    if(val == false)
    {
        cout << "\nNo word possible" << endl;
    }
    else
    {
        cout << "\nNext permutation : " << s << endl;
    }
    return 0;
}
