#include<iostream>
using namespace std;
int main()
{
    int num,a{0},b{1},next;
    cout << "Enter the fibonacci series : " ;
    cin >> num;
    cout << a << ", " << b << ", ";
    for(int i=2; i<=num; i++)
    {
        next = a + b;
        a = b;
        b = next;
        cout << next << ", ";
    }
    return 0;
}
