#include<iostream>
#include<string.h>
using namespace std;
int main()
{
    string strl;
    int k{0};
    cout << "Enter a string : ";
    getline(cin, strl);
    for(int i : strl)
    {
        k++;
    }
    cout << "After removing vowels : ";
    for(int i=0; i<k; i++)
    {
        if(strl[i] != 'a' && strl[i] != 'e'&& strl[i] != 'e' && strl[i] != 'i' && strl[i] != 'o' && strl[i] != 'u' && strl[i] != 'A' 
        && strl[i] != 'E' && strl[i] != 'I' && strl[i] != 'O' && strl[i] != 'U')
        {
            cout << strl[i];
        }
    }
    return 0;
}
