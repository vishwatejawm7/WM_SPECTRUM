#include<iostream>
using namespace std;
int main()
{
    int a,b,power{1};
    cout << "Enter the base : " ;
    cin >> a;
    cout << "Enter the exponent : ";
    cin >> b;
    for(int i = 1; i<=b; i++)
    {
        power = power * a;
    }
    cout << "The power value is : " << power << endl;
    return 0;
}
