#include<iostream>
using namespace std;
int main()
{
    int a,b;
    cout << "Enter the first number : ";
    cin >> a;
    cout << "Enter the second number : ";
    cin >> b;
    if(a==b)
    {
        cout << a << " is equal to " << b << endl;
    }
    else
    cout << max(a,b) << " is greater" << endl;
    return 0;
}
