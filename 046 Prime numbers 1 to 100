#include<iostream>
using namespace std;
int main()
{
    cout << "Prime numbers from 1 to  100\n";
    for(int i=1; i<=100; i++)
    {
        int count = 0;
        for(int j=1; j<=100; j++)
        {
            if(i%j == 0)
            {
                count += 1;
            }
        }
        if(count == 2)
        {
            cout << i << " ";
        }
    }
    return 0;
}
