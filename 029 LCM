#include<iostream>
using namespace std;
int main()
{
    int a,b,hcf,lcm;
    cout << "Program to find the lcm of two numbers " << endl;
    cout << "Enter the first number : ";
    cin >> a;
    cout << "Enter the second number : ";
    cin >> b;
    for(int i=1; i<a || i<b; i++)
    {
        if(a%i==0 && b%i==0)
        {
            hcf = i;
        }
    }
    lcm = (a*b) / hcf;
    cout << "The lcm of " << a << " & " << b << " is " << lcm << endl;
    return 0;
}
