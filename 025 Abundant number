#include<iostream>
using namespace std;
int main()
{
    int a, sum{0};
    cout << "Enter a number to check if abundand : ";
    cin >> a;
    for(int i=1; i<a; i++)
    {
        if(a%i == 0)
        {
            sum += i; 
        }
    }
    if(sum > a)
    {
        cout << a << " is abundand" << endl;
    }
    else
    {
        cout << a << " is not abundand" << endl;
    }
    return 0;
}
