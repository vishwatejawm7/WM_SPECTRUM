#include<iostream>
using namespace std;
int main()
{
    char a;
    cout << "Enter a character : ";
    cin >> a;
    if((a >= 'a' && a <= 'z') || (a >= 'A' && a <= 'Z'))
    {
        cout << "Yes " << a << " is an alphabet";
    }
    else
    {
        cout << "No " << a << " is not an alphabet";
    }
    return 0;
}
