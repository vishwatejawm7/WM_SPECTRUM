#include<iostream>
using namespace std;
int main()
{
    int a,b,c;
    cout << "Enter the first number : ";
    cin >> a;
    cout << "Enter the second number : ";
    cin >> b;
    cout << "Enter the third number : ";
    cin >> c;
    if(a==b && a==c && b==c)
    {
        cout << "All are equal" << endl;
    }
    else if(a>b && a>c)
    {
        cout << a << " is greatest " << endl;
    }
    else if(b>a && b>c)
    {
        cout << b << " is greatest " << endl;
    }
    else
    {
        cout << c << " is greatest " << endl;
    }
    return 0;
}
